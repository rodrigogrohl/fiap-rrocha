CERTIFICADO
keytool -importcert -alias twitter -file twitter-certificate-base64.cer -keystore clientkeystore
keytool -importcert -alias twitter2 -file twitter-certificate-base64.cer -keystore C:\Program Files\Java\jdk1.8.0_66\jre\lib\security\cacerts
keytool -import -trustcacerts -alias twitter -file twitter-api.cer -keypass 123456 -keystore clientkeystore -storepass 123456
